
public class DuplicateArray {


public static void main(String args[]) {
	int[] a= {2,3,7,2,8,4,8,9};
	
	System.out.println( "DUPLICATE VALUES ARE: ");
	for( int i=0;i<a.length;i++);{
		int i=0;
		for (int j=i+1;j<a.length;j++) 
		{
			if(a[i]==a[j] ) {
				System.out.println(a[i]);
			}
		 }
	}
	
}
}
