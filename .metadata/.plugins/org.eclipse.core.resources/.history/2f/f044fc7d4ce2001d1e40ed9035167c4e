package com.java;

public class Animal {
    public void speak() {
        System.out.println("The animal speaks");
    }
}

public class Dog extends Animal {
    @Override
    public void speak() {
        System.out.println("Woof!");
    }
}
public class Inheritance{
	public static void main(String args) {


Dog dog = new Dog();

dog.speak();
	}
}
