package com.java;

class Person{
	private int age;
	private String name;
	
	public Person() {
		age=12;
		name="Aruna";
	}
		public int getAge() {
	        return age;
		}
		public String getName() {
	        return name;
	    
	}
}




public class Constructor {
	 public static void main(String args[]) {
		 Person obj = new Person();
		 System.out.println(obj.getAge() + " : " +obj.getName());
		
	}

}
