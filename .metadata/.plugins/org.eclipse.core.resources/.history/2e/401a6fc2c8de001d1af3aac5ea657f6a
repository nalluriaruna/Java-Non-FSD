
 public class MaxMin {



	public static void main(String args, int i) {
		int a[]= {6,22,31,17,5,45};
  
		
		// to find maximum value 
 
		int max=a[0];
		for(int i1=0;i1<a.length;i1++);
		{
			
			
			if(a[i]>max)
			{
				max = a[i];
			}	
       System.out.println( "maximum element is " +max);
		}
	}
}

